<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project1o.StartpgController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@../Picture/startpgbg.jpg" />
         </image>
      </ImageView>
      <Pane layoutX="284.0" layoutY="67.0" prefHeight="297.0" prefWidth="286.0" style="-fx-background-color: brown;" AnchorPane.bottomAnchor="33.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="67.0">
         <children>
            <Button fx:id="signinbtn" layoutX="188.0" layoutY="215.0" mnemonicParsing="false" onAction="#signinbtnaction" text="Sign In" textAlignment="CENTER" textFill="#282121">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Button>
            <Text fill="#f5f4f4" layoutX="110.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign In" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Text>
            <TextField fx:id="usernamebox" layoutX="43.0" layoutY="77.0" onAction="#usernameboxactn" prefHeight="25.0" prefWidth="200.0" promptText="User name">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordbox" layoutX="44.0" layoutY="157.0" onAction="#passwordboxact" prefHeight="25.0" prefWidth="200.0" promptText="Password" />
            <Text fx:id="incrrecttxt" fill="#220202" layoutX="46.0" layoutY="211.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username or password is incorrect..!!" visible="false" />
            <Text fx:id="signuptxt" fill="#fcf8f8" layoutX="79.0" layoutY="265.0" onMouseClicked="#signupact" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have any account?" textAlignment="CENTER" underline="true">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="emailbox" layoutX="44.0" layoutY="117.0" prefHeight="25.0" prefWidth="200.0" promptText="E-mail">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </TextField>
         </children>
         <padding>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
         </padding>
      </Pane>
   </children>
</AnchorPane>
